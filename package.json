{
  "name": "nodejs",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",    // Start the bot using the compiled JavaScript
    "build": "tsc",                   // Compile TypeScript to JavaScript
    "dev": "ts-node index.ts"         // Use ts-node for development without compiling
  },
  "dependencies": {
    "@jup-ag/api": "^0.1.0",          // Jupiter API package
    "@solana/web3.js": "^1.46.0",     // Solana web3 package
