    "express": "^4.18.2",             // Express for handling webhooks
    "dotenv": "^10.0.0"               // dotenv for loading environment variables
  },
  "devDependencies": {
    "typescript": "^4.5.5",           // TypeScript
    "ts-node": "^10.4.0"              // ts-node for running TypeScript files directly
  }
}